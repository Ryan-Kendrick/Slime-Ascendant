{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/assets/*",
      "/*.js",
      "/*.css",
      "/fonts/*",
      "/*.woff",
      "/*.woff2",
      "/*.png",
      "/*.jpg",
      "/*.gif",
      "/*.svg"
    ]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  },
  "routes": [
    {
      "route": "/assets/static/*",
      "headers": {
        "cache-control": "public, max-age=14500"
      }
    },
    {
      "route": "/*",
      "rewrite": "/index.html"
    }
  ]
}
