{
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  },
  "routes": [
    {
      "route": "/assets/static/*",
      "headers": {
        "cache-control": "public, max-age=14500"
      }
    },
    {
      "route": "/*",
      "rewrite": "/index.html"
    }
  ]
}
